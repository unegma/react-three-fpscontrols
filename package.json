  "dependencies": {
    "nipplejs": "0.9.0",
    "@types/three": "^0.140.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "@react-three/fiber": ">=8.0",
