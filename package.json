{
  "name": "@unegma/react-three-fpscontrols",
  "version": "1.1.23",
  "description": "React Three Fiber FPS Controls for mobile and desktop",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "pub-major": "npm run build && npm version major --force && npm publish --access public && git push --tags",
    "pub-minor": "npm run build && npm version minor --force && npm publish --access public && git push --tags",
    "pub-patch": "npm run build && npm version patch --force && npm publish --access public && git push --tags",
    "unpublish-single-version": "npm unpublish <package-name>@<version>",
    "update-install": "npm update && npm install;",
    "build": "rm -rf ./dist && npm install && npx tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antonybudianto/react-three-fpscontrols.git"
  },
